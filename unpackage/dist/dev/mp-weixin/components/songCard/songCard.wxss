.song{
	margin-top: 30rpx;
}
.play-title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.play-front{
	font-size: 30rpx;
}
.play-after {
	font-size: 24rpx;
	padding: 10rpx 20rpx;
	-webkit-border-radius: 50rpx;
	        border-radius: 50rpx;
	border: 1rpx solid #ccc;
}
.iconmore {
	font-size: 20rpx;
}
.scroll-view {
	white-space: nowrap;
	width: 750rpx;
	margin-right: -20rpx;
}
.play-body {
	margin-top: 10rpx;
}
.play-body-outside {
	width: 95%;
	display: inline-block;
	/* border: 1rpx solid red; */
}
.play-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 10rpx;
}
.play-box-image {
	width: 150rpx;
	height: 150rpx;
	/* background-color: green; */
}
.play-box-img {
	width: 150rpx;
	height: 150rpx;
	-webkit-border-radius: 20rpx;
	        border-radius: 20rpx;
}
.play-box-title {
	width: 440rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin-left: 20rpx;
}
.play-box-title-top {
	/* background-color: red; */
}
.play-box-first-title {
	font-size: 30rpx;
}
.play-box-last-title {
	font-size: 24rpx;
	color: #ccc;
}
.song-info {
	font-size: 24rpx;
	color: #ccc;
}
.play-box-icon {
	width: 60rpx;
	height: 90rpx;
	line-height: 90rpx;
	text-align: center;
	font-size: 50rpx;
	margin: 30rpx;
}

