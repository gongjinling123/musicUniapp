<view><block wx:for="{{topList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="content-item"><view class="{{['item-left',index>2?'grey':'red']}}">{{''+(index+1)+''}}</view><view class="item-right"><view class="item-img"><image src="{{item.program.coverUrl}}"></image></view><view class="item-info"><view class="info-title">{{''+item.program.mainSong.name+''}}</view><view class="info-user"><view class="user-desc"><view class="user-img"><image src="{{item.program.dj.avatarUrl}}"></image></view><text>{{item.program.mainSong.artists[0].name}}</text></view><view class="num"><text class="iconfont icon-remen"></text>{{''+item.score+''}}</view></view></view><view class="item-play"><text class="iconfont icon-bofang1"></text></view></view></view></block></block></view>