.content-item{
	width: 100%;
	height: 120rpx;
	/* background-color: #007AFF; */
	margin-top: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.item-left{
	width: 70rpx;
	height: 120rpx;
	line-height: 120rpx;
	text-align: center;
	/* color: #FF3536; */
	font-weight: 600;
}
.red{
	color: red;
}
.grey{
	color: #929292;
}
.item-right{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.item-img{
	width: 120rpx;
	height: 120rpx;
}
.item-img image{
	width: 100%;
	height: 100%;
	-webkit-border-radius: 20rpx;
	        border-radius: 20rpx;
}
.item-info{
	width: 450rpx;
	margin-left: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.info-title{
	line-height: 70rpx;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	-webkit-box-flex: 2;
	-webkit-flex: 2;
	        flex: 2;
}
.info-user{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.user-desc{
	/* width: 200rpx; */
	margin-right: 20rpx;
	/* background-color: pink; */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.user-img{
	width: 40rpx;
	height: 40rpx;
}
.user-desc image{
	width: 100%;
	height: 100%;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
.user-desc text{
	max-width: 200rpx;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	font-size: 28rpx;
	color: #656565;
	margin-left: 10rpx;
}
.num{
	color: #656565;
	font-size: 26rpx;
	border-left: 2rpx solid #E9E9E9;
	padding-left: 30rpx;
}
.item-play{
	width: 90rpx;
	height: 120rpx;
	text-align: center;
	line-height: 120rpx;
}
.item-play text{
	font-size: 60rpx;
}

